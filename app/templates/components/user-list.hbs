{{#if currentUser.isAdmin}}
  <UserListAdmin @store={{store}} @currentUser={{currentUser}} @users={{users}} @organization={{organiztions}} @toUserInfo="toUserInfo" />
{{/if}}

{{#if currentUser.isTeacher}}
  <UserListTeacher @store={{store}} @currentUser={{currentUser}} @users={{users}} @organization={{organiztions}} @toUserInfo="toUserInfo" />
{{/if}}

{{#if currentUser.isPdAdmin}}
    <UserListPd @store={{store}} @currentUser={{currentUser}} @users={{users}} @organization={{organiztions}} @toUserInfo="toUserInfo" />
{{/if}}

{{#if currentUser.isStudent}}
    <div class="centered">FORBIDDEN</div>
{{/if}}


<div class="list-outlet users">
  {{outlet}}
</div>